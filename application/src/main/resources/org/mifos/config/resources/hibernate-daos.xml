<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="genericDao" class="org.mifos.accounts.savings.persistence.GenericDaoHibernate" />
    
    <bean id="applicationConfigurationDao" class="org.mifos.config.persistence.ApplicationConfigurationDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="fundDao" class="org.mifos.accounts.fund.persistence.FundDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="feeDao" class="org.mifos.accounts.fees.persistence.FeeDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="customerDao" class="org.mifos.customers.persistence.CustomerDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="personnelDao" class="org.mifos.customers.personnel.persistence.PersonnelDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="officeDao" class="org.mifos.customers.office.persistence.OfficeDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>

    <bean id="holidayDao" class="org.mifos.application.holiday.persistence.HolidayDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
   
    <bean id="loanDao" class="org.mifos.accounts.loan.persistance.LoanDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
     
    <bean id="loanProductDao" class="org.mifos.accounts.productdefinition.persistence.LoanProductDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="savingsProductDao" class="org.mifos.accounts.productdefinition.persistence.SavingsProductDaoHibernate" >
    	<constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="generalLedgerDao" class="org.mifos.accounts.financial.business.service.GeneralLedgerDaoHibernate" >
        <constructor-arg ref="genericDao" />
    </bean>
    
    <bean id="appliedUpgradesDao" class="org.mifos.application.admin.system.persistence.AppliedUpgradesDaoHibernate">
    	<constructor-arg ref="genericDao" />
    </bean>

    <bean id="questionDao" parent="abstractDao">
        <property name="proxyInterfaces" value="org.mifos.platform.questionnaire.persistence.QuestionDao"/>
        <property name="target">
            <bean parent="abstractDaoTarget">
                <constructor-arg>
                    <value>org.mifos.platform.questionnaire.domain.QuestionEntity</value>
                </constructor-arg>
            </bean>
        </property>
    </bean>

    <bean id="questionGroupDao" parent="abstractDao">
        <property name="proxyInterfaces" value="org.mifos.platform.questionnaire.persistence.QuestionGroupDao"/>
        <property name="target">
            <bean parent="abstractDaoTarget">
                <constructor-arg>
                    <value>org.mifos.platform.questionnaire.domain.QuestionGroup</value>
                </constructor-arg>
            </bean>
        </property>
    </bean>

    <bean id="eventSourceDao" parent="abstractDao">
        <property name="proxyInterfaces" value="org.mifos.platform.questionnaire.persistence.EventSourceDao"/>
        <property name="target">
            <bean parent="abstractDaoTarget">
                <constructor-arg>
                    <value>org.mifos.platform.questionnaire.domain.EventSourceEntity</value>
                </constructor-arg>
            </bean>
        </property>
    </bean>

    <bean id="questionGroupInstanceDao" parent="abstractDao">
        <property name="proxyInterfaces" value="org.mifos.platform.questionnaire.persistence.QuestionGroupInstanceDao"/>
        <property name="target">
            <bean parent="abstractDaoTarget">
                <constructor-arg>
                    <value>org.mifos.platform.questionnaire.domain.QuestionGroupInstance</value>
                </constructor-arg>
            </bean>
        </property>
    </bean>

    <bean id="sectionQuestionDao" parent="abstractDao">
        <property name="proxyInterfaces" value="org.mifos.platform.questionnaire.persistence.SectionQuestionDao"/>
        <property name="target">
            <bean parent="abstractDaoTarget">
                <constructor-arg>
                    <value>org.mifos.platform.questionnaire.domain.SectionQuestion</value>
                </constructor-arg>
            </bean>
        </property>
    </bean>
</beans>
