        <script-data-source name="MifosCollectionSheet" id="31">
            <method name="open"><![CDATA[logger = Packages.java.util.logging.Logger.getLogger("");

ALL_ID = new Packages.java.lang.Integer(-1);
SELECT_ID = new Packages.java.lang.Integer(-2);
NA_ID = new Packages.java.lang.Integer(-3);
userId = params["userId"];
CUSTOMER_LEVEL_GROUP  = 2;
CUSTOMER_LEVEL_CENTER = 3;
MAX_PRODUCT_COUNT = 2;
//user config variable: customerLevel
//set this to either CUSTOMER_LEVEL_CENTER or CUSTOMER_LEVEL_GROUP
customerLevel = CUSTOMER_LEVEL_CENTER;

customerLevelLabel = (customerLevel == CUSTOMER_LEVEL_CENTER) ? "Center" : "Group";

try {
	dateFormat = new Packages.java.text.SimpleDateFormat("dd/MM/yyyy");
    //collectionSheetService = new Packages.org.mifos.application.reports.business.service.CollectionSheetReportService();
    collectionSheetService = Packages.org.mifos.application.reports.business.service.ReportServiceFactory.getCacheEnabledCollectionSheetReportService();
} catch (e) {
	logger.info("MCSReport: Ds-open: exception : MifosScriptedDS : "+e);
}]]></method>
            <method name="close"><![CDATA[collectionSheetService = null;]]></method>
