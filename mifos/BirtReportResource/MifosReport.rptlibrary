            <property name="dataSource">MifosCollectionSheet</property>
            <method name="open"><![CDATA[collectionSheetReportsIterator = collectionSheetService.getCollectionSheets(params["branchId"],params["loanOfficerId"],
    params["centerId"],params["meetingDate"], customerLevel).iterator();]]></method>
            <method name="fetch"><![CDATA[if(!collectionSheetReportsIterator.hasNext()) return false;
var collectionSheetRow = collectionSheetReportsIterator.next();
row["member_id"] = collectionSheetRow.getCustomerId();
row["group_id"] = collectionSheetRow.getGroupId();
row["center_id"] = collectionSheetRow.getCenterId();
row["loan_officer_id"] = collectionSheetRow.getLoanOfficerId();
row["member_name"] = collectionSheetRow.getCustomerName();
row["branch_name"] = collectionSheetRow.getBranchName();
row["center_name"] = collectionSheetRow.getCenterName();
row["group_name"] = collectionSheetRow.getGroupName();
row["loan_officer_name"] = collectionSheetRow.getLoanOfficerName();
row["customer_level_label"] = customerLevelLabel;

for(var i=0;i<MAX_PRODUCT_COUNT;i++){
	var loanProduct = collectionSheetRow.getLoanProduct(i);
	row["loan_"+(i+1)+"_balance"] = loanProduct.getBalance();
	row["loan_"+(i+1)+"_due"] = loanProduct.getDue();
	row["loan_"+(i+1)+"_issues"] = loanProduct.getIssues();
	row["loan_"+(i+1)+"_name"] = loanProduct.getOfferingShortName();
}

for(var i=0;i<MAX_PRODUCT_COUNT;i++){
	var savingsProduct = collectionSheetRow.getSavingsProduct(i);
	row["savings_"+(i+1)+"_balance"] = savingsProduct.getBalance();
	row["savings_"+(i+1)+"_due"] = savingsProduct.getDue();
	row["savings_"+(i+1)+"_name"] = savingsProduct.getOfferingShortName();
}

row["other_fine"] = collectionSheetRow.getOtherFine();
row["other_fee"] = collectionSheetRow.getOtherFee();

return true;]]></method>
            <method name="close"><![CDATA[collectionSheetReportsIterator=null;]]></method>
