<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.meeting.util.valueobjects.MeetingRecurrence" table="RECUR_ON_DAY">
        <id name="detailsId" column="RECUR_ON_DAY_ID" type="integer">
             <generator class="native">
            </generator>
       </id>
	<version name="versionNo" column="VERSION_NO" type="integer" unsaved-value="null"/>
	
	<many-to-one name="meetingDetails" column="DETAILS_ID"  class="org.mifos.application.meeting.util.valueobjects.MeetingDetails"  insert="true" update="true"  unique="true" lazy="false" />
        <property name="weekDay" column="DAYS" type="short" />
        <property name="rankOfDays" column="RANK_OF_DAYS" type="short" />
        <property name="dayNumber" column="DAY_NUMBER" type="short" />
  
    </class>
</hibernate-mapping>