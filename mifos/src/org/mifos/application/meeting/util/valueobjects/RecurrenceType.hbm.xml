<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.meeting.util.valueobjects.RecurrenceType" table="RECURRENCE_TYPE">
        <id name="recurrenceId" column="RECURRENCE_ID" type="short">
            <generator class="native"/>
        </id>
         <property name="recurrenceName" column="RECURRENCE_NAME" type="string" />
         <property name="description" column="DESCRIPTION" type="string" />
 
    </class>
</hibernate-mapping>