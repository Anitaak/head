<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

	<class
		name="org.mifos.application.office.business.OfficeHierarchyEntity"
		table="OFFICE_HIERARCHY">
		<id name="hierarchyId" column="HIERARCHY_ID" type="integer" access="field">
			<generator class="native" />
		</id>
		<property name="createdDate" column="START_DATE" type="date" />
		<property name="endDate" column="END_DATE" type="date" />
		<property name="updatedDate" column="UPDATED_DATE" 	type="date"/>
        <property name="updatedBy" column="UPDATED_BY" />
        <property name="status" column="STATUS" />
 		<many-to-one name="office" class="org.mifos.application.office.business.OfficeBO" column="OFFICE_ID" unique="true" insert="true" update="true" lazy="true" cascade="none" />
		<many-to-one name="parentOffice" class="org.mifos.application.office.business.OfficeBO"	column="PARENT_ID" unique="true" insert="true"	update="true" lazy="true" cascade="none" />
	</class>
</hibernate-mapping>