<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.office.util.valueobjects.OfficeAddress" table="OFFICE_ADDRESS">
        <id name="officeAdressId" column="OFFICE_ADDRESS_ID" type="short">
            <generator class="native"/>
        </id>
 	
    <property name="address1" column="ADDRESS_1" type="string" />
    <property name="address2" column="ADDRESS_2" type="string" />
    <property name="address3" column="ADDRESS_3" type="string" />
    <property name="city" column="CITY" type="string" />
    <property name="state" column="STATE" type="string" />
    <property name="country" column="COUNTRY" type="string" />
    <property name="postalCode" column="ZIP" type="string" />
    <property name="telephoneNo" column="TELEPHONE" type="string" />        
    
    <many-to-one name="office" column="OFFICE_ID" class="org.mifos.application.office.util.valueobjects.Office"   />
    
    </class>

</hibernate-mapping>