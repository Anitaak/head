<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>
	<class name="org.mifos.application.office.util.valueobjects.OfficeCustomField" table="OFFICE_CUSTOM_FIELD">
        <id name="officecustomFieldId" column="OFFICE_CUSTOM_FIELD_ID" type="integer">
            <generator class="native"/>
        </id>
	
		<property name="fieldId" column="FIELD_ID" not-null="true"/>
	 	<property name="fieldValue" column="FIELD_VALUE" />
    
    	<many-to-one name="office" column="OFFICE_ID" class="org.mifos.application.office.util.valueobjects.Office" cascade="none" insert="true" update="true" unique="true"/>
	</class>
</hibernate-mapping>