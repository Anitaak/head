<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.productdefinition.util.valueobjects.PrdOfferingFees" table="PRD_OFFERING_FEES">
        <id name="prdOfferingFeeId" column="PRD_OFFERING_FEE_ID" >
            <generator class="native"/>
        </id>
 

 		 <many-to-one name="loanOffering" column="PRD_OFFERING_ID" class="org.mifos.application.productdefinition.util.valueobjects.LoanOffering" cascade="none" insert="true" update="true" unique="true"/>
        <many-to-one name="fees" column="FEE_ID" class="org.mifos.application.fees.util.valueobjects.Fees" cascade="none" insert="true" update="true" unique="true"/>
    </class>
    
</hibernate-mapping>
