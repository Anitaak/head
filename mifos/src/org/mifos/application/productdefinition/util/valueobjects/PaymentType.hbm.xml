<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.productdefinition.util.valueobjects.PaymentType" table="PAYMENT_TYPE">
        <id name="paymentTypeId" column="PAYMENT_TYPE_ID" type="short">
            <generator class="native"/>
        </id>
 
        <property name="lookUpId" column="PAYMENT_TYPE_LOOKUP_ID" type="integer"/>
    </class>
    
</hibernate-mapping>