<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping > 
  <class name="org.mifos.application.master.util.valueobjects.LookUpEntity" table="LOOKUP_ENTITY" >
  	<id column="ENTITY_ID" name="entityId" type="java.lang.Short" > 
		<generator class="native"></generator>
    </id>
    <property column="ENTITY_NAME"  name="entityType"  type="java.lang.String"/>
    
    <set name="lookUpLabelSet"  cascade="all" 	lazy="false">
            <key column="ENTITY_ID" update="false"  />
            <one-to-many   class="org.mifos.application.master.util.valueobjects.LookUpLabel"   />
    </set>
    <set name="lookUpValueSet"  cascade="all" 	lazy="false">
            <key column="ENTITY_ID" update="false"  />
            <one-to-many   class="org.mifos.application.master.util.valueobjects.LookUpValue"   />
    </set>	
	

  </class> 
  
    <query name="masterdata.entityvalue">
    	        	   <![CDATA[select new org.mifos.application.master.util.valueobjects.EntityMaster(entity.entityId ,label.localeId,label.labelName) from org.mifos.application.master.util.valueobjects.LookUpEntity entity, org.mifos.application.master.util.valueobjects.LookUpLabel label where entity.entityId = label.entityId and label.localeId=:localeId and entity.entityType=:entityType]]>
            </query>
            

            
</hibernate-mapping> 