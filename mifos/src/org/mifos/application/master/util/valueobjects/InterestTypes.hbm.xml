<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.master.util.valueobjects.InterestTypes" table="INTEREST_TYPES">
        <id name="interestTypeId" column="INTEREST_TYPE_ID" type="short">
            <generator class="native"/>
        </id>
 
        <property name="descripton" column="DESCRIPTON" type="string" />
 		<property name="lookUpId" column="LOOKUP_ID" type="integer" />
        <many-to-one name="productType" column="CATEGORY_ID" class="org.mifos.application.productdefinition.util.valueobjects.ProductType"  not-null="true" />
 
        <!--<many-to-one name="lookUpValue" column="LOOKUP_ID" class="org.mifos.application.master.util.valueobjects.LookUpValue"  not-null="true" />-->
    </class>
    <!-- <query name="product.getinteresttypes"><![CDATA[select new org.mifos.application.productdefinition.util.valueobjects.MasterObject(it.interestTypeId,lookupvalue.lookUpValue) from org.mifos.application.master.util.valueobjects.InterestTypes it,org.mifos.application.master.util.valueobjects.LookUpValueLocale lookupvalue  where it.productType.productTypeID=:productTypeID and it.lookUpValue.lookUpId=lookupvalue.lookUpId and lookupvalue.localeId=:localeId]]></query>-->
    
    <query name="product.getinteresttypes"><![CDATA[select new org.mifos.application.productdefinition.util.valueobjects.MasterObject(it.interestTypeId,lookupvalue.lookUpValue) from org.mifos.application.master.util.valueobjects.InterestTypes it,org.mifos.application.master.util.valueobjects.LookUpValueLocale lookupvalue  where it.productType.productTypeID=:productTypeID and it.lookUpId=lookupvalue.lookUpId and lookupvalue.localeId=:localeId]]></query>
</hibernate-mapping>
