<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.master.util.valueobjects.CollateralType" table="COLLATERAL_TYPE">
        <id name="collateralTypeId" column="COLLATERAL_TYPE_ID" type="short">
            <generator class="native"/>
        </id>
 
 
        <many-to-one name="lookUpId" column="LOOKUP_ID" class="org.mifos.application.master.util.valueobjects.LookUpValue"  not-null="true" unique="true"/>
 
       
    </class>
    
</hibernate-mapping>
