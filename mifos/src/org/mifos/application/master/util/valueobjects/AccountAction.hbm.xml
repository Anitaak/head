<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.master.util.valueobjects.AccountAction" table="ACCOUNT_ACTION">
        <id name="actionId" column="ACCOUNT_ACTION_ID" type="short">
            <generator class="native"/>
        </id>
 
        <many-to-one name="lookUpValue" column="LOOKUP_ID" class="org.mifos.application.master.util.valueobjects.LookUpValue" />
    </class>
    
</hibernate-mapping>
