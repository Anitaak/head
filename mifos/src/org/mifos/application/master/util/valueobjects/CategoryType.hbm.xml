<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.master.util.valueobjects.CategoryType" table="CATEGORY_TYPE">
        <id name="categoryId" column="CATEGORY_ID" type="short">
            <generator class="native"/>
        </id>
 
 
        <many-to-one name="lookUpEntity" column="CATEGORY_LOOKUP_ID" class="org.mifos.application.master.util.valueobjects.LookUpEntity"  not-null="true" />
 
        
    </class>
    
</hibernate-mapping>
