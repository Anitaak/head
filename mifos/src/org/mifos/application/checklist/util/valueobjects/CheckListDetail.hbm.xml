<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.checklist.util.valueobjects.CheckListDetail" table="CHECKLIST_DETAIL">
        <id name="detailId" column="DETAIL_ID" type="integer">
            <generator class="native"/>
        </id>
 
        <property name="detailText" column="DETAIL_TEXT" type="string" />
        <property name="answerType" column="ANSWER_TYPE" type="short"  not-null="true" />
 
        <many-to-one name="supportedLocales" column="LOCALE_ID" class="org.mifos.application.master.util.valueobjects.SupportedLocales" />
 
        <many-to-one name="checkList" column="CHECKLIST_ID" class="org.mifos.application.checklist.util.valueobjects.CheckList" cascade="none" />
    </class>
    
</hibernate-mapping>
