<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.personnel.business.PersonnelNotesEntity" table="PERSONNEL_NOTES">
        <id name="commentId" column="COMMENT_ID" type="integer">
            <generator class="native"/>
        </id>
 
        <property name="commentDate" column="COMMENT_DATE" type="date" />
        <property name="comment" column="COMMENTS" type="string" />
        <property name="officerId" column="OFFICER_ID" type="short" unique="false" update="true" insert="true" optimistic-lock="true" lazy="false" />
        <property name="personnelId" column="PERSONNEL_ID"  unique="false" update="true" insert="true" optimistic-lock="true" lazy="false" />
        <many-to-one  name="officer" column="OFFICER_ID" class="org.mifos.application.personnel.business.PersonnelBO" insert="false" update="false" unique="true" lazy="true"/>
        
	
    </class>
    
</hibernate-mapping>
