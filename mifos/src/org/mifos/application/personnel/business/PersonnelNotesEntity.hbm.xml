<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.personnel.business.PersonnelNotesEntity" table="PERSONNEL_NOTES">
        <id name="commentId" column="COMMENT_ID" type="integer" access="field">
            <generator class="native"/>
        </id>
 
        <property name="commentDate" column="COMMENT_DATE" type="date" access="field" />
        <property name="comment" column="COMMENTS" type="string" access="field"/>
        <many-to-one  name="officer" column="OFFICER_ID" class="org.mifos.application.personnel.business.PersonnelBO" insert="false" update="false" unique="true" lazy="true" access="field"/>
        <many-to-one  name="personnel" column="PERSONNEL_ID" class="org.mifos.application.personnel.business.PersonnelBO" insert="false" update="false" unique="true" lazy="true" access="field"/>
        
	
    </class>
    
</hibernate-mapping>
