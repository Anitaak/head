<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.personnel.business.PersonnelRoleEntity" table="PERSONNEL_ROLE">
        <id name="personnelRoleId" column="PERSONNEL_ROLE_ID" type="integer" unsaved-value="-1" access="field">
                <generator class="native"/>
        </id>
     	<many-to-one name="personnel" column="PERSONNEL_ID" class="org.mifos.application.personnel.business.PersonnelBO"  unique="true" access="field"/>
        <many-to-one  name="role"  column="ROLE_ID" class="org.mifos.application.rolesandpermission.util.valueobjects.Role" lazy="false" unique="true" access="field"/>
   </class>
</hibernate-mapping>
