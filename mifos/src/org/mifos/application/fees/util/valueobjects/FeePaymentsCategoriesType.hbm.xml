<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.fees.util.valueobjects.FeePaymentsCategoriesType" table="FEE_PAYMENTS_CATEGORIES_TYPE">
        <id name="feePaymentsCategoryTypeId" column="FEE_PAYMENTS_CATEGORY_TYPE_ID" type="short">
            <generator class="native"/>
        </id>
 
 
        <many-to-one name="feeType" column="FEE_TYPE_ID" class="org.mifos.application.fees.util.valueobjects.FeeType"  not-null="true" />
 
        <many-to-one name="categoryType" column="CATEGORY_ID" class="org.mifos.application.fees.util.valueobjects.CategoryType"  not-null="true" />
 
        <many-to-one name="feePayment" column="FEE_PAYMENT_ID" class="org.mifos.application.fees.util.valueobjects.FeePayment"  not-null="true" />
    </class>
    
</hibernate-mapping>
