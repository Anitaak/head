<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.fees.util.valueobjects.FeePayment" table="FEE_PAYMENT">
        <id name="feePaymentId" column="FEE_PAYMENT_ID" type="short">
            <generator class="native"/>
        </id>
 
 
        <property name="lookUpId" column="FEE_PAYMENT_LOOKUP_ID"  />
		  	
		        <set name="lookUpValueLocale" >
		          <key column ="LOOKUP_ID"
		           property-ref="lookUpId"/>
		          
		          <one-to-many  class="org.mifos.application.master.util.valueobjects.LookUpValueLocale" />
		        </set>
		         
		        
		    </class>
		        
		      <query name="masterdata.feepayment">
			        	   <![CDATA[select new org.mifos.application.master.util.valueobjects.FeePaymentMaster(payment.feePaymentId ,lookUp.lookUpValue) from org.mifos.application.fees.util.valueobjects.FeePayment  payment, org.mifos.application.master.util.valueobjects.LookUpValueLocale lookUp where payment.lookUpId = lookUp.lookUpId and lookUp.localeId=:localeId and payment.feePaymentId=:paymentId]]>
	          </query>
        
 
        

    
</hibernate-mapping>
