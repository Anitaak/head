<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.mifos.application.fees.business.FeeLevelEntity" table="FEELEVEL">
		<id name="feeLevelId" column="FEELEVEL_ID" type="short">
			<generator class="native" />
		</id>

		<property name="levelId" column="LEVEL_ID" />

		<many-to-one name="fee" column="FEE_ID" class="org.mifos.application.fees.business.FeesBO" cascade="all" insert="true" />
	</class>
</hibernate-mapping>
