<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.mifos.application.surveys.business.SurveyQuestion" table="SURVEY_QUESTIONS">
		<id name="sqId" column="SURVEYQUESTION_ID" type="integer" unsaved-value="null" access="field">
			<generator class="native" />
		</id>
		<property name="mandatory" column="MANDATORY" />
		<property name="questionOrder" column="QUESTION_ORDER"/>
		<many-to-one name="survey" column="SURVEY_ID"/>
		<many-to-one name="question" column="QUESTION_ID"/>
	</class>
</hibernate-mapping>

