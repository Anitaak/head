<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
 <class name="org.mifos.application.rolesandpermission.util.valueobjects.RoleActivity" table="ROLES_ACTIVITY">
	<composite-id name="compositeKey" class="org.mifos.application.rolesandpermission.util.valueobjects.RoleActivitiesKey"> 
	<key-property name="activityId" column="ACTIVITY_ID" type="short"/> 
	<key-property name="roleId" column="ROLE_ID" type="short"/> 
	</composite-id> 
	<version name="versionNo" column="VERSION_NO" type="integer">
               
        </version>
 </class>
</hibernate-mapping>
    