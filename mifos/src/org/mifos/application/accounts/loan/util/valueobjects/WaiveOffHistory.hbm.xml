<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.accounts.loan.util.valueobjects.WaiveOffHistory" table="WAIVE_OFF_HISTORY">
        <id name="waiveOffId" column="WAIVE_OFF_ID" type="integer">
            <generator class="native"/>
        </id>
        
        <property name="waiveOffDate" column="WAIVE_OFF_DATE" type="date"/>
        <property name="waiveOffType" column="WAIVE_OFF_TYPE" type="string"/>
        
        <many-to-one name="loan" column="ACCOUNT_ID" class="org.mifos.application.accounts.loan.util.valueobjects.Loan"/>
 
    </class>
    
</hibernate-mapping>
