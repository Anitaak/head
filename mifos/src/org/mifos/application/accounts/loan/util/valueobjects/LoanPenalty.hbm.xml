<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.accounts.loan.util.valueobjects.LoanPenalty" table="LOAN_PENALTY">
        <id name="loanPenaltyId" column="LOAN_PENALTY_ID" type="integer">
            <generator class="native"/>
        </id>
 
        <property name="startDate" column="START_DATE" type="date"   />
        <property name="endDate" column="END_DATE" type="date"   />
        <property name="penaltyType" column="PENALTY_TYPE" type="string"   />
        <property name="penaltyRate" column="PENALTY_RATE" type="double"   />
        <property name="penaltyId" column="PENALTY_ID" type="short"   />
 
        <many-to-one name="loan" column="ACCOUNT_ID" class="org.mifos.application.accounts.loan.util.valueobjects.Loan" />
 
        
    </class>
    
</hibernate-mapping>
