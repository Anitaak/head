<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >



<hibernate-mapping>

    <class name="org.mifos.application.customer.business.CustomerFlagEntity" table="CUSTOMER_FLAG_DETAIL">
        <id name="customerFlagId" column="CUSTOMER_FLAG_ID"  unsaved-value="-1">
            <generator class="native"/>
        </id>
    <version name="versionNo" column="VERSION_NO" type="integer" unsaved-value="null"/>
  	<property name="flagId" column="FLAG_ID"  />
  	<property name="createdBy" column="CREATED_BY"  />
  	<property name="createdDate" column="CREATED_DATE"  />
  	<many-to-one name="customer" column="CUSTOMER_ID" class="org.mifos.application.customer.business.CustomerBO"  insert="true" update="false" />
    </class>
        

</hibernate-mapping>
