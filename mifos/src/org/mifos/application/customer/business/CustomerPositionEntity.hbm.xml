<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.customer.business.CustomerPositionEntity" table="CUSTOMER_POSITION">
        <id name="customerPositionId" column="CUSTOMER_POSITION_ID" type="integer" unsaved-value="-1">
                <generator class="native"/>
        </id>
       <version name="versionNo" column="VERSION_NO" type="integer" unsaved-value="null"/>
       <many-to-one name="position" column="POSITION_ID" class="org.mifos.application.master.util.valueobjects.Position" insert="false" update="false"     lazy="true" />
	   <many-to-one name="customer" column="CUSTOMER_ID" class="org.mifos.application.customer.business.CustomerBO"  insert="false" update="false"     lazy="true"  />	
    </class>
</hibernate-mapping>
