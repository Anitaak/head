<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.framework.components.audit.util.valueobjects.AuditLog" table="CHANGE_LOG">
        <id name="logId" column="CHANGE_LOG_ID" type="integer" unsaved-value="null">
            <generator class="native"/>
        </id>
        <property name="featureId" column="ENTITY_ID" type="integer" />
        <property name="featureName" column="ENTITY_TYPE" type="short" />
        <property name="userName" column="CHANGED_BY" type="short" />
        <property name="updatedDate" column="CHANGED_DATE" type="date" />
        <property name="actualName" column="MODIFIER_NAME" type="string" />        
        <set name="auditLogRecords"  cascade="all" 	inverse="true" table="CHANGE_LOG_DETAIL">
            <key column="CHANGE_LOG_ID"  />
            <one-to-many   class="org.mifos.framework.components.audit.util.valueobjects.AuditLogRecord"   />
    	</set>	
       
       
    </class>
    
</hibernate-mapping>
