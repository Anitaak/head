<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.framework.components.audit.util.valueobjects.AuditLogRecord" table="CHANGE_LOG_DETAIL">
  		<id name="squenceNo" column="SEQUENCE_NUM" type="integer" unsaved-value="null">
            <generator class="native"/>
        </id>
  		<property name="fieldName" column="FIELD_NAME" type="string" />
        <property name="oldValue" column="OLD_VALUE" type="string" />
        <property name="newValue" column="NEW_VALUE" type="string" />
        <many-to-one  name="auditLog" column="CHANGE_LOG_ID" class="org.mifos.framework.components.audit.util.valueobjects.AuditLog"   lazy="true"/>
      
    </class>
    
</hibernate-mapping>
