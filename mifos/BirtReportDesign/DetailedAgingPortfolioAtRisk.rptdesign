	row["Report Date"] = reportData[count].getReportDate();
	row["Prepared By"] = reportData[count].getPrinter();
	row["Loan Number"] = reportData[count].getLoanNumber();
	row["Client"] = reportData[count].getClient();
	row["Loan Amount"] = reportData[count].getLoanAmount();
	row["Balance"] = reportData[count].getBalance();
	row["Principal"] = reportData[count].getPrincipal();
	row["Interest"] = reportData[count].getInterest();
	row["Arrears"] = reportData[count].getArrears();
	row["Current"]= reportData[count].getCurrent();
	row["1-30"] = reportData[count].getBalanceWhichDurationIsOneToThirty(reportData[count].getEclapsedDays());
	row["31-60"] = reportData[count].getBalanceWhichDurationIsThirtyOneToSixty(reportData[count].getEclapsedDays());
	row["61-90"]= reportData[count].getBalanceWhichDurationIsSixtyOneToNinety(reportData[count].getEclapsedDays());
	row["Current"]= reportData[count].getCurrent();
	
		return true;
}
return false;]]></method>
