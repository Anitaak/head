            <property name="queryText">Select 
	o.office_id as officeid, 
	p.prd_offering_name as prdofferingname, 
	sum(l.loan_amount) as loanamount 
from 
	office o
		left join 
			account a on a.office_id = o.office_id
		left join 
			loan_account l on l.account_id = a.account_id
		left join 
			prd_offering p on p.prd_offering_id = l.prd_offering_id
	WHERE 
			o.office_id = ?  
		and 
			date_format(l.disbursement_date,'%Y%m%d') = date_format(now(),'%Y%m%d') 
		and 
			a.account_state_id = 5
	group by 
		l.prd_offering_id</property>
