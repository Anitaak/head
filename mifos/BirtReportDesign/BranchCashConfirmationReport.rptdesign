            <property name="queryText">select 
	o.office_id as officeid,
	o.display_name as officename, 
	p.prd_offering_short_name as prdofferingname, 
	ifnull(sum(l.principal),0.000) as dueprincipal, 
	ifnull(sum(l.interest),0.000) as dueinterest , 
	ifnull(sum(l.misc_fees),0.000) as dueEF , 
	ifnull(sum(l.principal_paid),0.000) as actualprincipal, 
	ifnull(sum(l.interest_paid),0.000) as actualinterest, 
	ifnull(sum(l.misc_fees_paid),0.000) as actualEF,
	ifnull(sum(ifnull(l.principal,0.000) - ifnull(l.principal_paid,0.000)),0.000) as arrearsrprincipal, 
	ifnull(sum(ifnull(l.interest,0.000) - ifnull(l.interest_paid,0.000)),0.000) as arrearsinterest , 
	ifnull(sum(ifnull(l.misc_fees,0.000) - ifnull(l.misc_fees_paid,0.000)),0.000) as arrearsEF  
from 
	office o 
		left join 
			account a on a.office_id = o.office_id 
		left join 
			loan_account la on la.account_id = a.account_id 
		left join 
			loan_schedule l on l.account_id = a.account_id 
		left join 
			prd_offering p on p.prd_offering_id = la.prd_offering_id
	WHERE 
			o.office_id = ?
		and 
			date_format(l.action_date,'%Y%m%d') &lt;=date_format(now(),'%Y%m%d') 
		and 
			(weekday(l.action_date)= weekday(now())) 
		and 
			(date_format(l.payment_date,'%Y%m%d') = date_format(now(),'%Y%m%d') 
		or 
			isnull(l.payment_date) = true) 
		and
			(a.account_state_id = 5 or a.account_state_id = 9 
				or (a.account_state_id &lt;> 10 and date_format(a.closed_date,'%Y%m%d') =date_format(now(),'%Y%m%d')))
	group by 
		la.prd_offering_id		</property>
