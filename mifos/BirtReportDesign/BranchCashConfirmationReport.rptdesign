            <property name="queryText">Select 
	o.office_id as officeid, 
	ifnull(sum(cs.misc_fees_paid),0.000) as fines 
from 
		office o
	left join 
		customer c on c.branch_id = o.office_id
	left join 
		customer_schedule cs on cs.customer_id = c.customer_id
	WHERE 
		o.office_id = ?
	and 
		date_format(cs.payment_date,'%Y%m%d') = date_format(now(),'%Y%m%d') 
	and 
		cs.payment_status = 1
	group by 
		o.office_id</property>
