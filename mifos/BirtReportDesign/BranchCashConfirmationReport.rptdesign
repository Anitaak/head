            <property name="queryText">Select 
	o.office_id as offid,
	f.fee_name as Feetype, 
	sum(af.account_fee_amnt) as feeamount 
from 
		office o
	left join 
		account a on a.office_id = o.office_id
	left join 
		loan_account la on la.account_id = a.account_id
	left join 
		account_fees af on a.account_id = af.account_id
	left join 
		fees f on f.fee_id = af.fee_id
	WHERE 
			o.office_id = ?
		and 
			la.disbursement_date = date_format(now(),'%Y%m%d')
		and	
			a.account_state_id = 5
	group by 
		f.fee_id</property>
